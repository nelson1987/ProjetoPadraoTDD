@model WebForLink.Web.Areas.Administracao.Models.UsuarioAdministracaoModel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutAdministracao.cshtml";
}

<section class="content-header">
    <h1>Edição de @ViewBag.Title</h1>
    <ol class="breadcrumb">
        <li><i class="fa fa-envelope"></i> Acesso</li>
        <li class="active">@ViewBag.Title</li>
    </ol>
</section>
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header">
                    <h3 class="box-title">Dados do Usuário</h3>
                </div>
                <div class="box-body">
                    @Html.HiddenFor(model => model.Id)
                    <div class="row">
                        <div class="form-group">
                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.Nome, new { @class = "control-label" })
                                    @Html.DisplayFor(model => model.Nome, new { htmlAttributes = new { @class = "form-control" } })
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.Login, new { @class = "control-label " })
                                    @Html.DisplayFor(model => model.Login, new { htmlAttributes = new { @class = "form-control" } })
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.CPF, new { @class = "control-label " })
                                    @Html.DisplayFor(model => model.CPF, new { htmlAttributes = new { @class = "form-control" } })
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.DataNascimento, new {@class = "control-label "})
                                    @Html.DisplayFor(model => model.DataNascimento, new {htmlAttributes = new {@class = "form-control"}})
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.Email, new {@class = "control-label"})
                                    @Html.DisplayFor(model => model.Email, new {htmlAttributes = new {@class = "form-control"}})
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.Cargo, new {@class = "control-label"})
                                    @Html.DisplayFor(model => model.Cargo, new {htmlAttributes = new {@class = "form-control"}})
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.LoginSSO, new {@class = "control-label"})
                                    @Html.DisplayFor(model => model.LoginSSO, new {htmlAttributes = new {@class = "form-control"}})
                                </div>

                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.Dominio, new {@class = "control-label"})
                                    @Html.DisplayFor(model => model.Dominio, new {htmlAttributes = new {@class = "form-control"}})
                                </div>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            <input type="submit" value="Alterar" class="btn btn-default" />
                        </div>
                    </div>
                    <div>
                        @Html.ActionLink("Voltar", "GerenciarContasLst")
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
