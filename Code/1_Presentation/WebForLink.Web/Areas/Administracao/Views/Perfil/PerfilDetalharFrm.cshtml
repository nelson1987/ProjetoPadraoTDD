@model WebForLink.Web.Areas.Administracao.Models.PerfilAdministracaoModel

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdministracao.cshtml";
}


<section class="content-header">
    <h1>
        Perfil
        <!-- small>Control panel</!--small -->
    </h1>
    <ol class="breadcrumb">
        <li><i class="fa fa-cogs"></i> Administração</li>
        <li class="active">Perfil</li>
    </ol>
</section>


<!-- FORM -->
<section class="content">
    <div id="boxGrid" class="box box-primary">
        <div class="box-header">
            <h3 class="box-title">Solicitação</h3>
        </div>
        <div class="box-body">
            <div class="col-md-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.Nome)<br/>
                    @Html.DisplayFor(model => model.Nome)
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.Descricao)<br />
                    @Html.DisplayFor(model => model.Descricao)
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.Contratante.CNPJ)<br />
                    @Html.DisplayFor(model => model.Contratante.CNPJ)
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <div class="box box-primary">
        <div class="box-body">
            <div class="text-center text-nowrap">
                <button type="button" class="btn btn-default" onclick="location.href='@Url.Action("PerfilLst", "Perfil")'">Voltar</button>
            </div>
        </div>
    </div>
</section>