@model WebForLink.Web.Areas.Administracao.Models.AplicacaoAdministracaoModel

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdministracao.cshtml";
}


<section class="content-header">
    <h1>
        @ViewBag.Title
    </h1>
    <ol class="breadcrumb">
        <li><i class="fa fa-envelope"></i> @ViewBag.Page</li>
        <li class="active">@ViewBag.Title</li>
    </ol>
</section>
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header">
                    <h3 class="box-title">Dados da @ViewBag.Page</h3>
                </div>
                <div class="box-body">
                    <div class="col-md-6">
                        <div class="form-group">
                            @Html.LabelFor(model => model.Nome)<br />
                            @Html.DisplayFor(model => model.Nome)
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            @Html.LabelFor(model => model.Descricao)<br />
                            @Html.DisplayFor(model => model.Descricao)
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="box box-primary">
                <div class="box-body">
                    <div class="text-center text-nowrap">
                        <button type="button" class="btn btn-default" onclick="location.href='@Url.Action("AplicacaoLst", "Aplicacao")' ">Voltar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>