@model WebForLink.Web.ViewModels.DadosGeraisTesteVM


@using (Html.BeginCollectionItem("DadosGerais"))
{
    <div id="boxGrid" class="box box-primary">
        <div class="box-header">
            <h3 class="box-title"><a href="#demo" data-toggle="collapse">Dados Gerais</a></h3>
        </div>
        <div class="box-body collapse" id="demo">
            <div class="col-md-2">
                <div class="form-group">
                    @Html.LabelFor(model => model.Documento, htmlAttributes: new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.Documento, new { @class = "form-control input-sm", disabled = true })
                    @Html.ValidationMessageFor(model => model.Documento, "", new { @class = "text-danger" })
                </div>
            </div>
