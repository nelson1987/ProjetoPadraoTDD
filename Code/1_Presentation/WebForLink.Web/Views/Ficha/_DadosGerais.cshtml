@model WebForLink.Web.ViewModels.DadosGeraisVM
<div id="boxGridDadosGerais" class="box box-primary">
    <div class="box-header">
        <h3 class="box-title">Dados Gerais</h3>
    </div>
    <div class="box-body">
        <div class="col-md-3">
            <div class="form-group">
                @Html.LabelFor(model => model.Documento, htmlAttributes: new {@class = "control-label"})
                &nbsp;<span class="text-danger" style="font-weight: bold;">*</span><br/>
                @Html.TextBoxFor(model => model.Documento, new {@class = "form-control input-sm", disabled = true})
                @Html.ValidationMessageFor(model => model.Documento, "", new {@class = "text-danger"})
            </div>
        </div>
        <div class="clearfix"></div>
    </div>
</div>