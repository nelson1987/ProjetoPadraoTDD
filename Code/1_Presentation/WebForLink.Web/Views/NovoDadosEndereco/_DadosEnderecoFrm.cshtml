@model IEnumerable<WebForLink.Web.Controllers.NovaFichaCadastral.NovoDadosEnderecoVM>
<div class="box box-primary">
    <div class="box-header">
        <h3 class="box-title">Dados de Endereco</h3><span class="text-danger" style="font-weight:bold;">*</span>
        @Html.ValidationMessage("DadosEnderecosValidation", "", new { @class = "text-danger" })
    </div>
    <div class="box-body">
        <div id="divEnderecos">
            @Html.EditorForModel()
        </div>
        <div class="form-group" style="padding-top: 7px;">
            <button type="button" class="btn btn-default" onclick="incluirDados('DadosEnderecos', '@Url.Action("Adicionar", "NovoDadosEndereco")');" disable-input="">Adicionar Contato</button>
        </div>
        <div class="clearfix"></div>
    </div>
</div>